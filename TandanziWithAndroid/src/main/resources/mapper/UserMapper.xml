<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.scit.tandanzi.dao.UserMapper">
	<!--회원가입 -->
	<insert id="UserInsert" parameterType="User"> 
		insert into Users
		values(
		#{id}
		,#{pw}
		,#{weight}
		,#{carbohy}
		,#{protein}
		,#{fat}
		)
	</insert>
		
</mapper>
